                "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/nRF5-cmake-scripts/nRF5-cmake-toolchain.cmake",
                "JLINKRTTCLIENT": "C:/Program Files/SEGGER/JLink/JLinkRTTClient.exe",
                "JLINKGDBSERVER": "C:/Program Files/SEGGER/JLink/JLinkGDBServer.exe"
            }
        }
    ]
}